varnishtest "Test pupa vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import pupa from "${vmod_topbuild}/src/.libs/libvmod_pupa.so";

	sub vcl_deliver {
		set resp.http.a = example.push("a");
		set resp.http.a = example.push("a");
		set resp.http.a = example.push("a");
		set resp.http.b = example.push("b");
		set resp.http.b = example.push("b");
		set resp.http.c = example.push("c");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.a == 3
	expect resp.http.b == 2
	expect resp.http.c == 1
}

client c1 -run
